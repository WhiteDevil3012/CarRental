<div
  class="d-flex shadow-4 flex-wrap"
  style="background-color:white;margin-top:30px;margin-bottom:100px;margin-left:3%;margin-right:3%;"
>
<div class="table-responsive me-auto ms-auto">
<table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Image</th>
        <th scope="col">Rating</th>
        <th scope="col">Review</th>
      </tr>
    </thead>
    <tbody>
  {{#each feedbacks}}

      
        <tr class="table">
          <td>
            {{#if image}}
            <img src="{{image}}" style="width:100px;height:100px;">
            {{/if}}
            </td>
          <td>{{rating}}</th>
          <td>{{review}}</td>
        </tr>
    
  {{else}}
    <tr class="table"> <td>No feedback found</td></tr>

  {{/each}}
</tbody>
</table>
</div>
</div>

